                                        <div class="col-md-8">
                                            @Html.DropDownListFor(m => m.HtsNumberId, Model.SelectableHtsNumbers, new { @id = "htsNumberId", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-9 centered">
            <div class="panel panel-default">
