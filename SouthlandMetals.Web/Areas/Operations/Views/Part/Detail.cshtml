@model SouthlandMetals.Web.Areas.Operations.Models.PartViewModel

<div class="row" style="padding: 0;">
    <nav id="partsNav" class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav pull-right" style="padding-top: 6px;">
                    <li><button id="updatePartBtn" class="btn btn-sm btn-submit pull-right" title="Update Part" value="Update">Update</button></li>
                    <li><button id="cancelUpdatePartBtn" class="btn btn-sm btn-cancel pull-right" title="Cancel Update" value="Cancel Update">Cancel</button></li>
                </ul>
            </div>
        </div>
    </nav>
</div>

<div class="col-md-9 centered">
    <h4 class="text-noticed"></h4>
</div>

<form id="updatePartForm">
    <div class="row">
        <div class="col-md-12">
            <div class="panel-group">
                <div class="row">
                    <div class="col-md-9 centered">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <div class="clearfix">
                                    <h3 class="panel-title">
                                        Part Detail
                                    </h3>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        @if (Model.IsProjectPart)
                                        {
                                            <div class="col-md-12" style="margin-bottom:5px;">
                                                <span class="text-noticed col-md-12">Note: Temporary Part</span>
                                            </div>
                                        }
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(m => m.ProjectPartId, new { @id = "projectPartId", @class = "hidden" })
                                            @Html.TextBoxFor(m => m.PartId, new { @id = "partId", @class = "hidden" })
                                            @Html.TextBoxFor(m => m.AnnualUsage, new { @id = "annualUsage", @class = "hidden" })
                                            @Html.TextBoxFor(m => m.FoundryOrderId, new { @id = "foundryOrderId", @class = "hidden" })
                                            <div class="form-group">
                                                @Html.LabelFor(model => model.PartNumber, new { @class = "control-label col-md-3" })
                                                <div class="col-md-6">
                                                    @Html.TextBoxFor(model => model.PartNumber, new { @id = "partNumber", @class = "form-control", @required = "required" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                @Html.LabelFor(model => model.PartDescription, new { @class = "control-label col-md-3" })
                                                <div class="col-md-6">
                                                    @Html.TextBoxFor(model => model.PartDescription, new { @id = "partDescription", @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                        @if (!Model.IsProjectPart)
                                        {
                                            <div class="col-md-2">
                                                <div class="checkbox checkbox-success" style="margin-top: 2px;">
                                                    <input type="checkbox" id="isActive" readonly>
                                                    <label for="isActive" class="control-label col-md-5">
                                                        <strong>Active</strong>
                                                    </label>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-9 centered">
            <div class="panel panel-default">
                <div class="panel-heading parts" data-toggle="collapse" data-parent="#accordion" data-target="#collapseThree" style="background-color: white">
