                                            <div class="input-group">
                                                <span class="input-group-addon glyphicon glyphicon-usd"></span>
                                                @Html.TextBoxFor(model => model.PatternCost, new { @id = "patternCost", @class = "form-control", type = "number", min = 0 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.PatternPrice, new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <span class="input-group-addon glyphicon glyphicon-usd"></span>
                                                @Html.TextBoxFor(model => model.PatternPrice, new { @id = "patternPrice", @class = "form-control", type = "number", min = 0 })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        @Html.LabelFor(m => m.PatternMaterialId, new { @class = "control-label col-md-4" })
                                        <div class="col-md-8">
                                            @Html.DropDownListFor(m => m.PatternMaterialId, Model.SelectablePatternMaterial, new { @id = "patternMaterialId", @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group" style="padding-left: 15px;">
                                                <div class="checkbox checkbox-success">
                                                    <input type="checkbox" id="isFamilyPattern">
                                                    <label for="isFamilyPattern" class="control-label col-md-8">
                                                        <strong>Family Pattern</strong>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                @Html.LabelFor(m => m.ToolingOrderNumber, new { @class = "control-label col-md-4" })
                                                <div class="col-md-5">
                                                    @Html.TextBoxFor(m => m.ToolingOrderNumber, new { @id = "toolingOrderNumber", @class = "form-control", @readonly = "readonly" })
                                                </div>
                                            </div>
                                            <br /><br />
                                            <div class="form-group">
                                                @Html.LabelFor(m => m.ToolingDescription, new { @class = "control-label col-md-4" })
                                                <div class="col-md-8">
                                                    @Html.TextAreaFor(m => m.ToolingDescription, new { @id = "toolingDescription", @rows = "5", @cols = "65", @style = "font-size: 13px;overflow-y: scroll;" })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                @Html.LabelFor(m => m.Notes, new { @class = "control-label col-md-4" })
                                                <div class="col-md-8">
                                                    @Html.TextAreaFor(m => m.Notes, new { @id = "notes", @rows = "5", @cols = "65", @style = "font-size: 13px;overflow-y: scroll;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>

<div class="row">
    <div class="col-md-9 centered">
        <div class="panel panel-default">
            <div class="panel-heading parts" data-toggle="collapse" data-parent="#accordion" data-target="#collapseTwo">
                <h3 class="panel-title accordion-toggle">
                    Drawings
                    <i class="glyphicon glyphicon-menu-right glyphicon-menu-down pull-right panel-icon"></i>
                </h3>
            </div>
            <div id="collapseTwo" class="panel-collapse collapse in">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(m => m.RevisionNumber, new { @class = "control-label col-md-4" })
                                <div class="col-md-6">
                                    @Html.TextBoxFor(model => model.RevisionNumber, new { @id = "revisionNumber", @class = "form-control", @readonly = "readonly" })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding:10px;">
                        <div class="table-bordered" style="overflow-x:scroll; white-space:nowrap;">
                            <div id="drawingGallery" class="col-md-12">
                                @if (Model.Drawings != null)
                                {
                                    foreach (var drawing in Model.Drawings)
                                    {
                                        <div id="@drawing.DrawingId" style="display:inline-block; margin:0px 10px;">
                                            <a href="@Url.Action("GetDrawing", "Part", new { drawingId = @drawing.DrawingId, isProject = @drawing.IsProject })" target="_blank"><img src="@Url.Content("~/Content/images/southland_pdf_48.png")" class="img-responsive" alt="pdf" /></a>
                                            <div style="display: inline-block;">
                                                <span class="pull-left">@drawing.RevisionNumber</span>&nbsp;&nbsp;
                                                <span style="padding-top: 4px;" aria-hidden="true" class="glyphicon glyphicon-trash pull-right" onclick="deleteDrawing('@drawing.DrawingId');"></span>
                                            </div>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-4">
                            <form id="attachment" method="post" enctype="multipart/form-data" encoding="multipart/form-data">
                                <div class="input-group">
                                    @Html.HiddenFor(m => m.PartId)
                                    <input class="form-control" type="file" name="drawing" id="drawing" />
                                    <span class="input-group-btn">
                                        <button id="fileSubmitBtn" type="button" class="btn btn-sm btn-danger">Add</button>
                                    </span>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    var isActive = @Html.Raw(Json.Encode(Model.IsActive));
    var isRaw = @Html.Raw(Json.Encode(Model.IsRaw));
    var isMachined = @Html.Raw(Json.Encode(Model.IsMachined));
    var isFamilyPattern = @Html.Raw(Json.Encode(Model.IsFamilyPattern));
    var isProjectPart = @Html.Raw(Json.Encode(Model.IsProjectPart));
    var drawings = @Html.Raw(Json.Encode(Model.Drawings));
    var foundryOrderId = @Html.Raw(Json.Encode(Model.FoundryOrderId));
</script>

@section Scripts {
    @Scripts.Render("~/Scripts/southlandMetals-part-detail.js")
}



