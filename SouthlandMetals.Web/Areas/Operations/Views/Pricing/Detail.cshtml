                                <th>P/#</th>
                                <th>Machine Price</th>
                                <th>FOB Price</th>
                                <th>Tooling Price</th>
                                <th>Fixture Price</th>
                                <th>Add On</th>
                                <th>Surcharge</th>
                                <th>Duty</th>
                                <th>Price</th>
                                <th>Annual Price</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <td>Total</td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td>$@Model.TotalAnnualPrice</td>
                            </tr>
                        </tfoot>
                        <tbody data-bind="foreach: priceParts">
